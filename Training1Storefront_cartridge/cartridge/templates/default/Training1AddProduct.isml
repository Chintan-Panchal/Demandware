			<button id="${pdict.CurrentForms.training1addproduct.addProduct.htmlName}" type="submit" name="${pdict.CurrentForms.training1addproduct.addProduct.htmlName}" value="${Resource.msg('training1.addproduct.submit','forms',null)}">
