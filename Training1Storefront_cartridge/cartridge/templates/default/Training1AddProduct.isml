<iscontent type="text/html" charset="UTF-8" compact="true"/>
<isinclude template="util/modules"/>
<form action="${URLUtils.httpsContinue()}" method="POST" id="${pdict.CurrentForms.training1addproduct.htmlName}" class="form-horizontal">
	<div style="text-align:center">
		<div>
			<div>
				<isinputfield formfield="${pdict.CurrentForms.training1addproduct.training1_product_name}" type="input"/>
			</div>
		</div>
		<div>
			<div>
				<isinputfield formfield="${pdict.CurrentForms.training1addproduct.training1_product_price}" type="input"/>
